SECTION .TEXT
MAIN:
  PUSH  EBP
  MOV EBP, ESP
  CALL  FOO
  NOP
  POP EBP
  RET
  MOV EAX, DWORD PTR [ESP]
  RET
BAR:
  PUSH  EBP
  MOV EBP, ESP
  SUB ESP, 16
  MOV EDX, DWORD PTR 8[EBP]
  MOV EAX, DWORD PTR 12[EBP]
  ADD EAX, EDX
  MOV DWORD PTR -4[EBP], EAX
  NOP
  LEAVE
  RET
FOO:
  PUSH  EBP
  MOV EBP, ESP
  PUSH  222
  PUSH  111
  CALL  BAR
  ADD ESP, 8
  NOP
  LEAVE
  RET

